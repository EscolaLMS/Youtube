{
  "name": "escolalms/youtube",
  "description": "Escola LMS youtube integration.",
  "type": "package",
  "require": {
    "php": ">=7.4",
    "laravel/framework": "^8.0|^9.0",
    "escolalms/core": "^1",
    "escolalms/auth": "^0",
    "escolalms/settings": "^0",
    "google/apiclient": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Hubert Krzysztofiak",
      "email": "hubert.krzysztofiak@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\Youtube\\": "src",
      "EscolaLms\\Youtube\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\Youtube\\EscolaLmsYoutubeServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
